#include "stm32f7xx.h"
#include "W25N512GVEIG.h"
#include <string.h>

extern QSPI_HandleTypeDef hqspi;

HAL_StatusTypeDef flash_get_id(void) {
	uint8_t id; // we will just get the manufacturer ID not device

	QSPI_CommandTypeDef com;

	com.Instruction = GET_ID;
	com.InstructionMode = QSPI_INSTRUCTION_1_LINE;
	com.DummyCycles = 8;

	HAL_QSPI_Command(&hqspi, &com, 100);

}
